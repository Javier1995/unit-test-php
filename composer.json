{
    "name": "javier/bug-report-app",
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "Src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\":"Tests/"
        }        
    },
    "scripts": {
        "test": [
            "phpunit --colors=always --testdox --verbose --configuration phpunit.xml"
        ]
    }
}
